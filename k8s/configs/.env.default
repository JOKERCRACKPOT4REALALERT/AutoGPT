# AutoGPT Platform Configuration for Kubernetes
# This file contains environment variables that MUST be set for the AutoGPT platform
# Copy this to .env.local and update the values

## ===== REQUIRED DATABASE CONFIGURATION ===== ##
# PostgreSQL Database Connection (Supabase PostgreSQL in-cluster)
DB_USER=supabase
DB_PASS=CHANGE-THIS-TO-A-SECURE-PASSWORD
DB_NAME=supabase  
DB_PORT=5432
DB_HOST=supabase-postgresql.autogpt.svc.cluster.local
DB_CONNECTION_LIMIT=12
DB_CONNECT_TIMEOUT=60
DB_POOL_TIMEOUT=300
DB_SCHEMA=platform
DATABASE_URL="postgresql://${DB_USER}:${DB_PASS}@${DB_HOST}:${DB_PORT}/${DB_NAME}?schema=${DB_SCHEMA}&connect_timeout=${DB_CONNECT_TIMEOUT}"
DIRECT_URL="postgresql://${DB_USER}:${DB_PASS}@${DB_HOST}:${DB_PORT}/${DB_NAME}?schema=${DB_SCHEMA}&connect_timeout=${DB_CONNECT_TIMEOUT}"
PRISMA_SCHEMA="postgres/schema.prisma"
ENABLE_AUTH=true

## ===== GCP CONFIGURATION ===== ##
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1

